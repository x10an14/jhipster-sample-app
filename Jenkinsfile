pipeline {
  agent any
  stages {
    stage('Build Backend') {
      steps {
        sh 'echo \'Trolol\''
      }
    }
    stage('Test Backend') {
      steps {
        echo 'trololol'
      }
    }
    stage('More Tests') {
      steps {
        sh 'echo \'Tests should be done!\''
      }
    }
    stage('Deploy Staging') {
      steps {
        sh 'echo \'Deploying to Staging..\''
      }
    }
    stage('Deploy Production') {
      steps {
        echo 'Deployiing to Production!'
      }
    }
  }
}